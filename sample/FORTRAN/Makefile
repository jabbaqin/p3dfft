	   echo " $(INSTALL_PROGRAM_ENV) $(fsamplePROGRAMS_INSTALL) '$$p' '$(DESTDIR)$(fsampledir)/$$f'"; \
	   $(INSTALL_PROGRAM_ENV) $(fsamplePROGRAMS_INSTALL) "$$p" "$(DESTDIR)$(fsampledir)/$$f" || exit 1; \
	  else :; fi; \
	done

